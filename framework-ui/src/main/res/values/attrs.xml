<?xml version="1.0" encoding="utf-8"?>
<resources>
    <item name="offset_helper" type="id" />
    <attr name="parallaxMode">
        <enum name="parallax" value="0" />
        <enum name="parallax_below_toolbar" value="1" />
        <enum name="pin" value="2" />
    </attr>
    <attr name="titleTransitionMode">
        <enum name="fade" value="0" />
        <enum name="move" value="1" />
    </attr>
    <declare-styleable name="ParallaxToolbarLayout">
        <attr name="parallaxMultiplier" format="float"/>
        <attr name="parallaxMode"/>
        <attr name="titleTransitionMode"/>
        <attr name="scrim" format="reference"/>
        <attr name="statusScrim" format="reference"/>
        <attr name="titleTextView" format="reference"/>
        <attr name="floatingToolbar" format="reference"/>
        <attr name="floatingToolbarTitleTextView" format="reference"/>
    </declare-styleable>
    <declare-styleable name="InsetLayout">
        <attr name="statusBarColor" format="color"/>
    </declare-styleable>
</resources>